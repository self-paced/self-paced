{
  "name": "sls",
  "version": "0.1.0",
  "main": "serverless.ts",
  "private": true,
  "scripts": {
    "dev": "source .env && sls offline --stage local --httpPort 5050",
    "prisma:migrate": "npx prisma migrate dev --name init",
    "deploy:dev": "source .env.dev && sls deploy --stage dev",
    "deploy:prd": "source .env.prd && sls deploy --stage prd",
    "postinstall": "prisma generate"
  },
  "engines": {
    "node": ">=16.15.1"
  },
  "prisma": {
    "schema": "prisma/schema.prisma"
  },
  "dependencies": {
    "@line/bot-sdk": "7.5.0",
    "@prisma/client": "4.1.0",
    "@trpc/server": "9.25.3",
    "@vendia/serverless-express": "4.10.1",
    "aws-sdk": "2.1160.0",
    "axios": "0.27.2",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "express": "4.18.1",
    "source-map-support": "0.5.21",
    "zod": "3.17.3"
  },
  "devDependencies": {
    "@babel/core": "7.18.5",
    "@babel/preset-env": "7.18.2",
    "@babel/preset-typescript": "7.17.12",
    "@serverless/typescript": "3.19.0",
    "@types/aws-lambda": "8.10.101",
    "@types/aws-sdk": "2.7.0",
    "@types/cookie-parser": "1.4.3",
    "@types/cors": "2.8.12",
    "@types/node": "18.0.4",
    "@types/webpack-node-externals": "2.5.3",
    "babel-loader": "8.2.5",
    "copy-webpack-plugin": "11.0.0",
    "fork-ts-checker-webpack-plugin": "7.2.11",
    "prisma": "4.1.0",
    "serverless": "3.19.0",
    "serverless-offline": "8.8.0",
    "serverless-webpack": "5.7.1",
    "tsconfig-paths": "4.0.0",
    "tsconfig-paths-webpack-plugin": "3.5.2",
    "typescript": "4.7.4",
    "webpack": "5.73.0",
    "webpack-node-externals": "3.0.0"
  }
}